{
    "name": "tiriel/matching-bundle",
    "type": "library",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "version": "v2.0.0",
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/doctrine-bundle": "^2.18",
        "symfony/cache-contracts": "^3.6",
        "symfony/dependency-injection": "^7.3",
        "symfony/flex": "^2",
        "symfony/messenger": "^7.3",
        "symfony/string": "^7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "symfony/browser-kit": "^7.3",
        "symfony/css-selector": "^7.3"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Tiriel\\MatchingBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tiriel\\MatchingBundle\\Tests\\": "tests/"
        }
    }
}
